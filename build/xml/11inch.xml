<?xml version="1.0" encoding="UTF-8"?>
<GMXML>
    <!-- Left Indicator Telltale -->
    <Telltale id="LeftIndicator" value="1"/>   <!-- 
        id: Unique identifier for the telltale (used in your code to match) 
        value: 1 = ON, 0 = OFF. This controls whether the left indicator should flash.
    -->

    <!-- Right Indicator Telltale -->
    <Telltale id="RightIndicator" value="1"/>  <!-- 
        Similar to LeftIndicator:
        value = 1 turns it ON (can be static or flashing depending on your code logic)
    -->
</GMXML>
